{
  "extends": ["next/core-web-vitals", "next/typescript","eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-unused-vars": "error", //disallows unused cariables and functions
    "no-console": "error",
    "prefer-const": "error", //Disallows let if variables are not reassigned
    "quotes": "error", //enforces the use of double quotes over single quotes
    "semi": "error", //Enforces semicolons at the end of statements
    "no-multiple-empty-lines": ["error", {"max":1}], //Disallows multiple empty lines
    "no-extra-semi": "error", //Disallows unnecessary semicolons
    "camelcase": "error", // Enforces camelCase naming convention
    "react/no-array-index-key":"warn",
    "react/react-in-jsx-scope": "off",
    "no-undef": "off",
    "@typescript-eslint/no-explicit-any": "off"
  },
  "plugins": ["react"],
  
  "settings":{
    "react": {
      "version":"detect"
    }
  }
}
